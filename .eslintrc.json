{
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "quotes": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "singleQuote": true,
    "jsx-quotes": 0
  },
  "extends": [
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/react",
    "prettier/standard"
  ]
}